{
    "manifest_version": 2,
    "author": "plug³ Team",
    "name": "plugCubed",
    "short_name": "plug³",
    "description": "Adding a new dimension to plug.dj",
    "homepage_url": "https://plugCubed.net",
    "version": "4.4.0.4",

    "icons": {
        "48": "icons/48.png",
        "64": "icons/64.png",
        "128": "icons/128.png"
    },

    "content_scripts": [{
        "matches": ["https://*.plug.dj/*"],
        "exclude_matches": [
            "*://*/",
            "*://*/*@*/*",
            "*://*/*_*/*",
            "*://*/*about*",
            "*://*/*ba*",
            "*://*/*dashboard*",
            "*://*/*jobs*",
            "*://*/*logout*",
            "*://*/*partners*",
            "*://*/*plot*",
            "*://*/*press*",
            "*://*/*privacy*",
            "*://*/*purchase*",
            "*://*/*subscribe*",
            "*://*/*team*",
            "*://*/*terms*"
        ],
        "js": ["loader.js"]
    }],

    "permissions": [
        "https://plug.dj/*"
    ],

    "web_accessible_resources": [
        "plugCubed.js"
    ]
}
